{
  "name": "clapy",
  "version": "1.0.0",
  "description": "Figma plugin to start building Clapy applications.",
  "private": true,
  "license": "UNLICENSED",
  "___sideEffects": [
    "*.css",
    "*.scss",
    "*.jpg",
    "*.svg",
    "*.png",
    "*.gif",
    "*.webp",
    "*.zip"
  ],
  "scripts": {
    "start": "NODE_ENV=development APP_ENV=development ts-node src/scripts/esbuild/serve.ts",
    "start0": "yarn webpackdev",
    "dev": "npm-run-all --parallel dev-with-browser:plugin dev-with-browser:browser dev-with-browser:server",
    "_build:staging": "NODE_ENV=production APP_ENV=staging ts-node src/scripts/esbuild/build.ts",
    "build:prod": "yarn build-step1 && NODE_ENV=production APP_ENV=production ts-node src/scripts/esbuild/build.ts && yarn build-step2",
    "_build:staging0": "yarn build-step1 && yarn webpackbuild:staging && yarn build-step2",
    "build:prod0": "yarn build-step1 && yarn webpackbuild:prod && yarn build-step2",
    "up": "cd .. && yarn up",
    "upgr": "yarn upgrade-interactive --latest",
    "_________________ implementation details _________________": "",
    "dev-with-browser:browser": "APP_ENV=dev PREVIEW_ENV=browser vite",
    "dev-with-browser:plugin": "PREVIEW_ENV=figma NODE_ENV=development APP_ENV=development ts-node src/scripts/esbuild/serve.ts",
    "dev-with-browser:plugin0": "yarn webpackdev --env PREVIEW_ENV=figma",
    "dev-with-browser:browser0": "yarn webpackdevserver --env PREVIEW_ENV=browser",
    "dev-with-browser:server": "nodemon --inspect=0.0.0.0:9230 --watch src/core/dev-preview-mode/preview-server.js src/core/dev-preview-mode/preview-server.js",
    "clean": "sudo kill -9 `sudo lsof -t -i:9001` && sudo kill -9 `sudo lsof -t -i:9000`",
    "lint": "eslint src --ext .js,.jsx,ts,tsx --fix",
    "gentypes": "cd .. && yarn gentypes",
    "____ webpack command. If updating one, you probably should update the other. ____": "",
    "webpackdevserver": "NODE_ENV=development webpack-dev-server --mode=development --env APP_ENV=dev --env PREVIEW_ENV=browser",
    "webpackdev": "NODE_ENV=development webpack --mode=development --env APP_ENV=dev --watch",
    "webpackbuild:staging": "NODE_ENV=production webpack --mode=production --env APP_ENV=staging",
    "webpackbuild:prod": "NODE_ENV=production webpack --mode=production --env APP_ENV=production",
    "wpstats": "NODE_ENV=production webpack --mode=production --env APP_ENV=production --profile --json > stats.json",
    "____ end webpack command ____": "",
    "build-step1": "rimraf dist && mkdirp dist/build && ncp manifest.json dist/manifest.json",
    "build-step2": "ncp build-prod/index.html dist/build/index.html && ncp build-prod/backend.js dist/build/backend.js && cd dist && bestzip plugin.zip *",
    "analyzebundle": "yarn webpackbuild:staging --profile --json > stats.json",
    "killbyport": "sudo kill -9 `sudo lsof -t -i:9001`"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.8.0",
    "@mui/lab": "^5.0.0-alpha.83",
    "@mui/material": "^5.8.1",
    "@reduxjs/toolkit": "^1.8.2",
    "@types/react-lottie": "^1.2.6",
    "buffer": "^6.0.3",
    "fast-deep-equal": "^3.1.3",
    "jwt-decode": "^3.1.2",
    "libphonenumber-js": "^1.10.11",
    "magic-bytes.js": "^1.0.12",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-lottie": "^1.2.3",
    "react-phone-input-material-ui": "^2.17.4",
    "react-redux": "^8.0.2",
    "react-toastify": "^9.0.1",
    "sha.js": "^2.4.11"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@figma/plugin-typings": "^1.49.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/node": "^16",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@types/sha.js": "^2.4.0",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@vitejs/plugin-react": "^1.3.2",
    "babel-loader": "^8.2.5",
    "babel-preset-react-app": "^10.0.1",
    "bestzip": "^2.2.1",
    "css-loader": "^6.7.1",
    "csstype": "^3.1.0",
    "dotenv": "^16.0.1",
    "esbuild": "^0.14.42",
    "esbuild-css-modules-plugin": "^2.2.16",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "fork-ts-checker-webpack-plugin": "^7.2.11",
    "html-webpack-plugin": "^5.5.0",
    "mkdirp": "^1.0.4",
    "ncp": "^2.0.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "react-dev-utils": "^12.0.1",
    "react-refresh": "0.13.0",
    "react-refresh-typescript": "^2.0.5",
    "rimraf": "^3.0.2",
    "sass": "^1.52.1",
    "sass-loader": "^13.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2",
    "url-loader": "^4.1.1",
    "vite": "^2.9.9",
    "webpack": "^5.72.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0",
    "ws": "^8.7.0"
  }
}
