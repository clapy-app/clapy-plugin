{
  "name": "backend-clapy",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "start": "yarn up",
    "dev": "rm -rf dist-dev && swc src -s -d dist-dev && npm-run-all --parallel dev:swc:watch dev:nodemon",
    "dev2": "yarn dev:clean && yarn dev:swc && npm-run-all --parallel dev:swc:watch dev:nodemon2",
    "dev:debug": "yarn dev --debug 0.0.0.0:9229",
    "dev:debug2": "yarn dev --debug 0.0.0.0:9229 --config nest-cli-main2.json",
    "start:docker": "yarn && yarn dev",
    "start:docker:debug": "yarn && yarn dev:debug",
    "start:docker:debug:main2": "yarn && yarn dev:debug2",
    "start:prod": "node dist/main",
    "up": "cd .. && yarn up",
    "stop": "cd .. && yarn stop",
    "log": "cd .. && yarn log",
    "upgr": "yarn upgrade-interactive",
    "build": "yarn build:clean && swc src -d dist",
    "build-old": "nest build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "check-puppeteer": "node src/core/check-puppeteer.js",
    "analytics": "ts-node --transpile-only src/scripts/analytics/calc-anaytics.ts",
    "_________________ implementation details _________________": "",
    "lint": "eslint \"{src,test}/**/*.{js,ts}\" --fix",
    "dev:clean": "rimraf dist-dev",
    "dev:swc": "swc src -s -d dist-dev",
    "dev:swc:watch": "swc src -w -s -d dist-dev",
    "dev:nodemon": "nodemon --watch \"dist-dev/**/*.js\" --exec \"node --enable-source-maps dist-dev/main.js\" --delay 0.1",
    "dev:nodemon2": "nodemon --watch \"dist-dev/**/*.js\" --exec \"node --enable-source-maps dist-dev/main2.js\" --delay 0.1",
    "build:clean": "rimraf dist && rimraf node_modules/.cache/tsconfig.tsbuildinfo",
    "_________________ old _________________": "",
    "dev-old": "nest start --watch",
    "dev2-old": "nest start --watch --config nest-cli-main2.json"
  },
