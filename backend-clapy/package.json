    "auth0": "^2.42.0",
    "axios": "^0.27.2",
    "css-tree": "^2.2.0",
    "expr-eval": "^2.0.2",
    "express": "^4.18.1",
    "express-jwt": "6.1.2",
    "express-rate-limit": "^6.5.1",
    "express-sanitizer": "^1.0.6",
    "fast-deep-equal": "^3.1.3",
    "hbs": "^4.2.0",
    "helmet": "^5.1.1",
    "jsonwebtoken": "^8.5.1",
